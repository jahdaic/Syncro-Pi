{
	"rewrites": [
		{ "source": "/Syncro-Pi", "destination": "/index.html"},
		{ "source": "/Syncro-Pi/static/:type/:file", "destination": "/static/:type/:file"},
		{ "source": "/Syncro-Pi/:file.:type", "destination": "/:file.:type"},
